extends layouts/main
include mixins/videoBlock

block content
    .home-videos
        each item in videos
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl
            })

            //- +를 붙이는 이유는 pug의 함수를 호출할때는 +를 붙임